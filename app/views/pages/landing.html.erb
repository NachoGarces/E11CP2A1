<table class='table table-hover'>
  <thead>
    <tr>
      <th scope='col'>Name</th>
      <th scope='col'>Email</th>
      <th scope='col'>Age</th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr class='table table-light' scope="row">
        <td><%= student.name %></td>
        <td><%= student.email %></td>
        <td><%= student.age %></td>
      </tr>
    <% end %>
  </tbody>
</table>
